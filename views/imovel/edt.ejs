<%- include('../cabecalho.ejs') %>
<form method="post" enctype="multipart/form-data">
    <br><br>
    ID<br>
    <input type="text" name="id" readonly value="<%=Imovel._id%>">
    <br><br>
    Cidade<br> 
    <input type="text" name="cidade" placeholder="Digite a cidade do imovel.">
    <br><br>
    Rua<br>
    <input type="text" name="rua" placeholder="Digite a rua do imovel.">
    <br><br>
    Localização<br>
    <input type="text" name="localizacao" placeholder="Digite a Localização do imovel.">
    <br><br>
    Numero<br>
    <input type="number" name="numero" placeholder="Digite o numero do imovel.">
    <br><br>
    Bairro<br>
    <input type="text" name="bairro" placeholder="Digite o bairro do imovel.">
    <br><br>
    Foto<br>
    <input type="file" name="foto">
    <br><br>
    Cliente<br>
    <select class="form-select" name="cliente" aria-label="Selecione um usuário!">
        <option value="1">One</option>
    </select>
    <input type="submit" value="Adicionar">
</form>
<%- include('../rodape.ejs') %>